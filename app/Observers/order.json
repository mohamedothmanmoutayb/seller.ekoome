                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"network\": 1,// Get from /networks\n    \"store\": \"store1\",// Get from /stores\n    \"mode\": 1,// 1 for standard order\n    \"status\":0,// 0 for add and not send orders, 1 for add & send  orders\n    \"orders\": [\n        {\n            \"num\": \"commande 01\",\n            \"type\": 1,  // 1 for normal delivery,\n            \"name\": \"Abdo\",\n            \"phone\": \"0728597208\",\n            \"address\": \"Agadir\",\n            \"city\": \"Agadir\",\n            \"price\": 35,\n            \"refs\": [\n                {\n                    \"designation\": \"test designation\",\n                    \"quantity\": 1\n                }\n            ],\n            \"port\": 1,  // 1 fees paid by seller, 2 fees ,\n            \"openproduct\": 1,  // 1 for allow open product, 2 Fees paid in advance,\n            \"note\": \"livraision matin\"\n        },\n        {\n            \"num\": \"commande 02\",\n            \"type\": 1,\n            \"name\": \"Abdo\",\n            \"phone\": \"0728597208\",\n            \"address\": \"Agadir\",\n            \"city\": \"Agadir\",\n            \"price\": 35,\n            \"refs\": [\n                {\n                    \"designation\": \"test designation\",\n                    \"quantity\": 1\n                }\n            ],\n            \"port\": 1,\n            \"note\": \"livraision matin\"\n        }\n    ]\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{baseUrl}}/orders",
                            "host": [
